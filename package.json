{
  "name": "novel-support.js",
  "description": "jQuery非依存の、小説投稿サイト代替記号の変換ライブラリ",
  "version": "1.0.0",
  "author": "WindChimeYK",
  "bugs": {
    "url": "https://github.com/windChimeYK/novel-support.js/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-remove-export": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "lynt": "^0.2.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3"
  },
  "files": [
    "src",
    "dist",
    "LICENSE.txt",
    "README.md",
    "package.json"
  ],
  "homepage": "https://github.com/windChimeYK/novel-support.js#readme",
  "license": "ISC",
  "main": "./dist/js/index.js",
  "unpkg": "./dist/js/novel-support.js",
  "jsdelivr": "./dist/js/novel-support.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windChimeYK/novel-support.js.git"
  },
  "scripts": {
    "babel": "babel",
    "sass": "node-sass",
    "build:npm": "babel --compact=true ./src/novel-support.js -o ./dist/js/index.js",
    "build:cdn": "babel --compact=true ./src/novel-support.js -o ./dist/js/novel-support.js --plugins babel-plugin-transform-remove-export",
    "build:sass": "node-sass ./src/_novel-support.scss ./dist/css/novel-support.css --output-style compressed",
    "build": "npm-run-all -p build:*",
    "watch": "babel ./src/novel-support.js -w -o ./dist/js/index.js",
    "lint": "lynt ./src/novel-support.js"
  }
}
